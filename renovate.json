{
  "extends": [
    "config:base",
    ":rebaseStalePrs",
  ],
  "addLabels": [
    "renovate"
  ],
  "dependencyDashboard": true,
  "dependencyDashboardAutoclose": true,
  "lockFileMaintenance": {
    "enabled": true,
    "automerge": true,
    "extends": [
      "schedule:weekly"
    ]
  },
  "major": {
    "matchConfidence": "high",
    "automerge": false
  },
  "minor": {
    "matchConfidence": "high",
    "automerge": true
  },
  "patch": {
    "matchConfidence": "high",
    "automerge": true
  },
  "pin": {
    "matchConfidence": "high",
    "automerge": true
  },
  "docker": {
    "enabled": false,
    "additionalBranchPrefix": "docker-"
  },
  "packageRules": [
    {
      "matchConfidence": "high",
      "automerge": true,
      "extends": [
        "packages:linters"
      ]
    },
    {
      "matchConfidence": "high",
      "automerge": true,
      "extends": [
        "packages:test"
      ]
    },
    {
      "matchConfidence": "high",
      "automerge": true,
      "matchPackagePrefixes": [
        "@types/"
      ]
    }
  ],
  "ignoreTests": false,
  "automergeType": "pr",
  "separateMinorPatch": false,
  "prCreation": "not-pending",
  "prConcurrentLimit": 10,
  "prHourlyLimit": 0,
  "vulnerabilityAlerts": {
    "enabled": true
  }
}
